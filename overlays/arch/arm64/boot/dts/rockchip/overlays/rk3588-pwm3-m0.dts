/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM3-M0";
		compatible = "radxa,nx5-io";
		category = "misc";
		exclusive = "GPIO3_B2";
		description = "Enable PWM3-M0.\nOn Radxa NX5 IO this is pin 3.";
	};

	fragment@0 {
		target = <&pwm3>;

		__overlay__ {
			status = "okay";
			pinctrl-0 = <&pwm3m0_pins>;
		};
	};
};
