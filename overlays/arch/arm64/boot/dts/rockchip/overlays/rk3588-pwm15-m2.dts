/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM15-M2";
		compatible = "radxa,cm5-io";
		category = "misc";
		exclusive = "GPIO1_C6";
		description = "Enable PWM15-M2.\nOn Radxa CM5 IO this is pin 36.";
	};

	fragment@0 {
		target = <&pwm15>;

		__overlay__ {
			status = "okay";
			pinctrl-0 = <&pwm15m2_pins>;
		};
	};
};
